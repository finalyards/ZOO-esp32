
macro_rules! pins {
    ($io:ident) => {{
        use esp_hal::{ gpio::{Output as O, Level as L} };

        let x: (_, _, Option<O>,[O;2]) = (  // esp32c6
            $io.pins.gpio18,
            $io.pins.gpio19,
            Some(O::new($io.pins.gpio20, L::Low)),
            [O::new($io.pins.gpio21, L::Low), O::new($io.pins.gpio22, L::Low)]
        );
        x
    }}
}